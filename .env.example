# OpenPace Configuration Example
# Copy this file to .env and customize for your environment

# Database
DATABASE_PATH=./openpace_data.db

# Anonymization
DEFAULT_ANONYMIZATION=false

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/openpace.log

# Data directories
DATA_IMPORT_PATH=./data/import
DATA_EXPORT_PATH=./data/export

# Privacy
STRIP_PII_ON_EXPORT=true
